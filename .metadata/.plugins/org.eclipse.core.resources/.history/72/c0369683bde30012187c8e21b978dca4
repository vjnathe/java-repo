package com.xcompany.almodule;

import java.io.ObjectInputStream.GetField;

class ArrayListDemo {
	
	private long id,age;
	private String name,college;
	

	public long getId() {
		return id;
	}


	public void setId(long id) {
		this.id = id;
	}


	public long getAge() {
		return age;
	}


	public void setAge(long age) {
		this.age = age;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public String getCollege() {
		return college;
	}


	public void setCollege(String college) {
		this.college = college;
	}

    public static void main(String[] args) {
		
       ArrayListDemo persons[] =new ArrayListDemo[5];
       
       for (int i = 0; i < persons.length; i++) {
       persons[i]=new ArrayListDemo();
	   persons[i].setId(i+1);
		
	}
       persons[0].setName("Vijayraj");
       persons[0].setCollege("MIT");
       persons[0].setAge(24);
       persons[0].setName("Anvay");
       persons[0].setCollege("SCOE");
       persons[0].setAge(21);
       persons[0].setName("Rana");
       persons[0].setCollege("");
       persons[0].setAge(21);
       persons[0].setName("Swaroop");
       persons[0].setCollege("MITCOE");
       persons[0].setAge(21);
       persons[0].setName("Akash");
       persons[0].setCollege("MITCOE");
       persons[0].setAge(20);
	}
}
